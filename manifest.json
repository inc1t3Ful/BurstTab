{
  "name": "Burst Tab",
  "description": "Open multiple tabs in a burst",
  "version": "1.0",

  "manifest_version": 2,

  "icons":{
    "128": "icons128.png",
    "48": "icons48.png",
    "16": "icons16.png"
  },

  "browser_action":{
    "default_icon": "icon16.png",
    "default_popup": "popup.html",
    "default_title": "Burst Tab"
  },

  "permissions": [
    "tabs",
    "storage"
  ]


}
